{
    "active": true,
    "exclude": ["express", "elasticsearch", "node_modules", "monitor"],
    "createTempCopyOfInstrumention": true,
    "nameAnonymousFunctions": false,
    "useGivenNames": true,
    "compressPosts": true,
    "active_debug_service_type": "file",
    "debug_services":[
    {
        "type": "service",
        "url": "ec2-54-189-147-119.us-west-2.compute.amazonaws.com",
        "url_getFunctions": "/OfflineDebugger/CodeSelection/Functions",
        "url_postData": "/OfflineDebugger/DebugData/Data",
        "username": "seffy",
        "password": "seffy"
    },
    {
        "type": "file",
        "path": "../config/debug_configuration.json",
        "outputLog": "debug"
    }],
    "autoCheckConfiguration" : {
        "once": false,
        "every": 10000
    },
    "logging": {
        "globalLogLevel": "info",
        "levels": {
            "verbose": 1,
            "info": 2,
            "remote": 3,
            "data": 4,
            "warn": 5,
            "debug": 6,
            "error": 7
        },
        "colors": {
            "verbose": "cyan",
            "info": "green",
            "data": "gray",
            "warn": "yellow",
            "debug": "blue",
            "error": "red",
            "remote": "white"
        },
        "debug": {
            "log": true,
            "logToConsole": true,
            "logToFile": true,
            "filename": "debug",
            "roll_file": true,
            "roll_everyday": false
        },
        "exceptions": {
            "log": true,
            "logToConsole": true,
            "logToFile": false,
            "filename": "exception",
            "roll_file": true,
            "roll_everyday": false
        }
    }
}